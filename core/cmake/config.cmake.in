set (@CMAKE_PROJECT_NAME@_VERSION @PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_PATCH@)

@PACKAGE_INIT@

foreach(ARILES_COMPONENT  core yaml-cpp octave ros array rapidjson)
    find_package(ariles2-${ARILES_COMPONENT} REQUIRED)

    list(APPEND @CMAKE_PROJECT_NAME@_LIBRARIES     "${ariles2-${ARILES_COMPONENT}_LIBRARIES}")
    list(APPEND @CMAKE_PROJECT_NAME@_INCLUDE_DIRS  "${ariles2-${ARILES_COMPONENT}_INCLUDE_DIRS}")
    list(APPEND @CMAKE_PROJECT_NAME@_LIBRARY_DIRS  "${ariles2-${ARILES_COMPONENT}_LIBRARY_DIRS}")
endforeach()
