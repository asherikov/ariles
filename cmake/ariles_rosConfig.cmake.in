set (@CMAKE_PROJECT_NAME@_VERSION @PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_PATCH@)

@PACKAGE_INIT@

foreach(ARILES_COMPONENT  core yaml-cpp octave ros array)
    find_package(ariles-${ARILES_COMPONENT} REQUIRED)

    list(APPEND @CMAKE_PROJECT_NAME@_LIBRARIES     "${ariles-${ARILES_COMPONENT}_LIBRARIES}")
    list(APPEND @CMAKE_PROJECT_NAME@_INCLUDE_DIRS  "${ariles-${ARILES_COMPONENT}_INCLUDE_DIRS}")
endforeach()
